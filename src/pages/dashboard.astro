---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import { DashboardICV } from '@/dashboard/Dashboard';
const robots = ["index", "follow"];
const title = "Dashboard";
const description = "Dashboard disponibles para estudiantes y profesionales.";
const keywords = ["Dashboard", "Instituto de Capacitación y Desarrollo Virtual", "Capacitación", "Desarrollo Virtual", "Estudiantes", "Profesionales"];
const url = "https://icv-procesos-academicos-frontend.vercel.app/dashboard";
const imageOG = "/incadev_isologotipo.webp";
const imagetwt = "/incadev_isologotipo.webp";
---

<DashboardLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
	<DashboardICV client:load/>
</DashboardLayout>
