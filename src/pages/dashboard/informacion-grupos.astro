---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import InformationGroup from '@/dashboard/groups/information/InformationGroup';
const robots = ["index", "follow"];
const title = "Informacion de Grupos";
const description = "Informacion de Grupos disponibles para estudiantes y profesionales.";
const keywords = ["Informacion de Grupos", "Instituto de Capacitación y Desarrollo Virtual", "Capacitación", "Desarrollo Virtual", "Estudiantes", "Profesionales"];
const url = "https://instituto.cetivirgendelapuerta.com/academico/dashboard/informacion-grupos";
const imageOG = "/academico/incadev_isologotipo.webp";
const imagetwt = "/academico/incadev_isologotipo.webp";
---

<DashboardLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
	<InformationGroup client:load/>
</DashboardLayout>
