---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import ClassView from '@/dashboard/courses/class/Class';
const robots = ["index", "follow"];
const title = "Clase";
const description = "Modulo Educativo disponibles para estudiantes y profesionales.";
const keywords = ["Modulo Educativo", "Instituto de Capacitación y Desarrollo Virtual", "Capacitación", "Desarrollo Virtual", "Estudiantes", "Profesionales"];
const url = "https://instituto.cetivirgendelapuerta.com/academico/dashboard/courses/clase";
const imageOG = "/academico/incadev_isologotipo.webp";
const imagetwt = "/academico/incadev_isologotipo.webp";
---

<DashboardLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
	<ClassView client:load/>
</DashboardLayout>
