---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import EducationModule from '@/dashboard/courses/modulo-educativo/EducationModule';
const robots = ["index", "follow"];
const title = "Modulo Educativo";
const description = "Modulo Educativo disponibles para estudiantes y profesionales.";
const keywords = ["Modulo Educativo", "Instituto de Capacitación y Desarrollo Virtual", "Capacitación", "Desarrollo Virtual", "Estudiantes", "Profesionales"];
const url = "https://icv-procesos-academicos-frontend.vercel.app/dashboard/catalogo";
const imageOG = "/incadev_isologotipo.webp";
const imagetwt = "/incadev_isologotipo.webp";
---

<DashboardLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
	<EducationModule client:load/>
</DashboardLayout>
