---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import { Catalog } from '@/dashboard/catalog/Catalog';
const robots = ["index", "follow"];
const title = "Cursos";
const description = "Cursos disponibles para estudiantes y profesionales.";
const keywords = ["Cursos", "Instituto de Capacitación y Desarrollo Virtual", "Capacitación", "Desarrollo Virtual", "Estudiantes", "Profesionales"];
const url = "https://icv-procesos-academicos-frontend.vercel.app/dashboard/catalogo";
const imageOG = "/incadev_isologotipo.webp";
const imagetwt = "/incadev_isologotipo.webp";
---

<DashboardLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
	<Catalog client:load/>
</DashboardLayout>
