---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import ManagementGroup from '@/dashboard/groups/management/ManagementGroup';
const robots = ["index", "follow"];
const title = "Gestión de Grupos";
const description = "Gestión de Grupos disponibles para estudiantes y profesionales.";
const keywords = ["Gestión de Grupos", "Instituto de Capacitación y Desarrollo Virtual", "Capacitación", "Desarrollo Virtual", "Estudiantes", "Profesionales"];
const url = "https://instituto.cetivirgendelapuerta.com/academico/dashboard/gestion-grupos";
const imageOG = "/academico/incadev_isologotipo.webp";
const imagetwt = "/academico/incadev_isologotipo.webp";
---

<DashboardLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
	<ManagementGroup client:load/>
</DashboardLayout>
