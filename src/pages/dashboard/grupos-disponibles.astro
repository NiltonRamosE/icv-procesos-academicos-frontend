---
import DashboardLayout from '@/layouts/DashboardLayout.astro';
import GruposDisponiblesPage from '@/dashboard/grupos-disponibles/GruposDisponibles';
const robots = ["index", "follow"];
const title = "Grupos Disponibles";
const description = "Grupos disponibles para el curso seleccionado.";
const keywords = ["Grupos", "Cursos", "Instituto de Capacitación y Desarrollo Virtual", "Capacitación", "Desarrollo Virtual", "Estudiantes", "Profesionales"];
const url = "https://icv-procesos-academicos-frontend.vercel.app/dashboard/grupos-disponibles";
const imageOG = "/incadev_isologotipo.webp";
const imagetwt = "/incadev_isologotipo.webp";
---

<DashboardLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
